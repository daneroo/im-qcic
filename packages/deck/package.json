  "description": "Static front end (mdx-deck) for qcic",
  "author": "Daniel Lauzon <daniel.lauzon@gmail.com>",
  "homepage": "https://github.com/daneroo/im-qcic#readme",
  "license": "ISC",
  "private": true,
  "//publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daneroo/im-qcic.git"
  },
  "bugs": {
    "url": "https://github.com/daneroo/im-qcic/issues"
  },
  "scripts": {
    "start": "mdx-deck deck.mdx",
    "build": "mdx-deck build deck.mdx",
    "deploy": "npm run build && npm run deploy:now",
    "deploy:now": "cp now.json dist/ && cd dist && now --public --target production && now rm --safe --yes deck-qcic",
    "testWithAuditLater": "npm run lint && npm audit && npm run unit",
    "test": "npm run lint && npm run unit",
    "lint": "standard",
    "unit": "jest",
    "coverage": "jest --collectCoverageFrom=lib/**.js --coverage ."
  },
  "dependencies": {
    "@daneroo/qcic-apollo-client-setup": "^1.0.15",
    "@daneroo/qcic-react": "^1.0.28",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-link-ws": "^1.0.17",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "moment": "^2.24.0",
    "react-apollo": "^2.5.3",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "devDependencies": {
    "jest": "^26.0.1",
    "mdx-deck": "^2.0.5",
    "standard": "^14.3.4"

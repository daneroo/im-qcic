{
  "name": "@daneroo/qcic-site",
  "version": "1.0.40",
  "description": "Main QCIC static site (gatsby with theme)",
  "author": "Daniel Lauzon <daniel.lauzon@gmail.com>",
  "homepage": "https://github.com/daneroo/im-qcic#readme",
  "license": "MIT",
  "private": true,
  "//publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daneroo/im-qcic.git"
  },
  "bugs": {
    "url": "https://github.com/daneroo/im-qcic/issues"
  },
  "scripts": {
    "dev": "gatsby develop",
    "start": "gatsby build && gatsby serve",
    "build": "gatsby build",
    "NOT-YET-NEEDED-vercel-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && npm run build",
    "deploy:now": "now --prod && now rm --safe --yes site-qcic",
    "testWithAuditLater": "npm run lint && npm audit && npm run unit",
    "test": "npm run lint && npm run unit",
    "lint": "standard",
    "unit": "echo jest when appropriate",
    "coverage": "jest --collectCoverageFrom=lib/**.js --coverage ."
  },
  "devDependencies": {
    "gatsby": "^2.29.1",
    "gatsby-theme-document": "^1.0.10",
    "jest": "^26.6.3",
    "standard": "^16.0.3"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@apollo/link-ws": "^2.0.0-beta.3",
    "@daneroo/qcic-gql-client": "^1.0.39",
    "@daneroo/qcic-react": "^1.0.38",
    "moment": "^2.29.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "subscriptions-transport-ws": "^0.9.18"
  }
}

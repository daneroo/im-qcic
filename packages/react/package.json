{
  "name": "@daneroo/qcic-react",
  "version": "1.0.1",
  "description": "QCIC Reusable components",
  "author": "Daniel Lauzon <daniel.lauzon@gmail.com>",
  "homepage": "https://github.com/daneroo/im-qcic#readme",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daneroo/im-qcic.git"
  },
  "bugs": {
    "url": "https://github.com/daneroo/im-qcic/issues"
  },
  "main": "dist/qcic-react.js",
  "umd:main": "dist/qcic-react.umd.js",
  "module": "dist/qcic-react.m.js",
  "source": "src/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "standard",
    "test": "jest --verbose --coverage --silent",
    "test:watch": "jest --watch --runInBand --silent",
    "prebuild": "npm run lint && npm t && rimraf dist",
    "build": "microbundle -o dist/ --sourcemap true --target web --jsx React.createElement",
    "dev": "microbundle watch -o dist/ --sourcemap true --target web --jsx React.createElement --compress false"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "graphql": "^14.2.0",
    "graphql-tag": "^2.10.1",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "microbundle": "^0.9.0",
    "react": "^16.8.0",
    "react-apollo": "^2.5.3",
    "react-dom": "^16.8.0",
    "react-testing-library": "^5.5.0",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1"
  },
  "peerDependencies": {
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "graphql": "^14.2.0",
    "graphql-tag": "^2.10.1",
    "react": "^16.8.0",
    "react-apollo": "^2.5.3"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "dependencies": {
    "@use-it/event-listener": "^0.1.2"
  },
  "standard": {
    "//parser": "babel-eslint",
    "env": {
      "jest": true
    }
  }
}
